xterm*font: -*-fixed-medium-*-*-*-14-*-*-*-*-*-*-*
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true
URxvt*saveLines: 8192
!URxvt*font: -*-fixed-medium-*-*-*-14-*-*-*-*-*-*-*
!URxvt.font: 9x15
!URxvt.font: xft:Terminus:antialias=false:size=8
!URxvt*font: xft:DejaVu Sans Mono for Powerline:size=12
!URxvt*boldFont: xft:DejaVu Sans Mono for Powerline:bold:size=12
URxvt.font: -*-fixed-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt.foreground: white
URxvt.background: black
URxvt.scrollBar: false
URxvt.color12: rgb:5c/5c/ff
URxvt.iso14755: False
! https://github.com/muennich/urxvt-perls
URxvt.perl-ext-common: ...,keyboard-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
